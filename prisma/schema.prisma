
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Document {
  id        String   @id @default(cuid())
  content   String
  embedding Unsupported("vector(1536)")?
  metadata  Json?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}